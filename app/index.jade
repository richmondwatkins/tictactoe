doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    title Tic-Tac-Toe
    link(rel='stylesheet', href='./css/app.css')
  body.body



div#header
  button#reset Reset / New Game
  h1 Facebook vs Twitter
  h5 (tic tac toe)


    table#board
      tbody
        - for(var i = 0; i < 3; i++)
          tr
            - for(var j = 0; j < 3; j++)
              td.table(data-x=j, data-y=i)


    script(src='./js/vendor/jquery-2.1.0.js')
    script(src='./js/source/main.js')
